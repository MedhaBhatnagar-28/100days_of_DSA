#include <stdio.h>

int main() {
    int n, k, i;
    
    // Read size of array
    scanf("%d", &n);

    int arr[n];

    // Read array elements
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Read key to search
    scanf("%d", &k);

    int comparisons = 0;
    int found = 0;

    // Linear Search
    for(i = 0; i < n; i++) {
        comparisons++;

        if(arr[i] == k) {
            printf("Found at index %d\n", i);
            found = 1;
            break;
        }
    }

    if(!found) {
        printf("Not Found\n");
    }

    printf("Comparisons = %d\n", comparisons);

    return 0;
}
